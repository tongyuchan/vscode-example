{
	"name": "vscode-example-tyc",
	"displayName": "vscode-example-tyc",
	"description": "",
	"version": "0.0.1",
	"publisher": "tongyuchan",
	"engines": {
		"vscode": "^1.49.0"
	},
	"icon": "",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./client/out/extension",
	"repository": {
		"type": "git",
		"url": "https://github.com/tongyuchan/vscode-example.git"
	},
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "vscode-example-tyc",
			"properties": {
				"vscode-example-tyc.warning": {
					"scope": "resource",
					"type": "boolean",
					"default": true,
					"description": "是否开启warning提示"
				}
			}
		}
	},
	"scripts": {
		"postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
		"vscode:prepublish-todo": "npm run webpack",
		"vscode:prepublish": "npm run clean && npm run compile && webpack --mode production --config ./client/webpack.config.js",
		"webpack": "npm run clean && webpack --mode production --config ./client/webpack.config.js & webpack --mode production --config ./server/webpack.config.js",
		"clean": "rimraf client/out && rimraf server/out",
		"compile": "tsc -b",
		"lint": "eslint src --ext ts",
		"watch": "tsc -b -w",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12",
		"ts-loader": "^6.2.1",
		"merge-options": "^2.0.0",
		"rimraf": "^3.0.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	}
}
